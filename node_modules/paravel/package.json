{
  "name": "paravel",
  "version": "0.1.11",
  "description": "Yet another toolkit for nostr",
  "repository": {
    "type": "git",
    "url": "https://github.com/coracle-social/paravel.git"
  },
  "main": "./dist/paravel.cjs",
  "module": "./dist/paravel.esm.js",
  "exports": {
    "import": "./dist/paravel.esm.js",
    "require": "./dist/paravel.cjs"
  },
  "scripts": {
    "build": "node build.js",
    "pub": "npm i && npm run check && node build.js && npm publish",
    "check:ts": "tsc --noEmit --esModuleInterop --strict src/**/*",
    "check:es": "eslint src/*",
    "check": "run-p check:*"
  },
  "keywords": [
    "nostr"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.0.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "esbuild": "0.16.9",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-babel": "^5.3.1",
    "esm-loader-import-relative-extension": "^1.0.8",
    "esm-loader-typescript": "^1.0.3",
    "node-esm-loader": "^0.0.3",
    "prettier": "2.8.3",
    "tsd": "^0.22.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "isomorphic-ws": "^5.0.0",
    "nostr-tools": "^1.7.5",
    "npm-run-all": "^4.1.5"
  }
}
