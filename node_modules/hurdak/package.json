    "babel-register": "^6.26.0",
    "chai": "3.5.0",
    "eslint": "^4.6.1",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "npm-run-all": "^4.1.1",
    "regenerator-runtime": "^0.13.9",
